#include <iostream>

int main() {
    double length, width, height;
    
    std::cout << "Обчислення об'єму паралелепіпеда.\n";
    
    std::cout << "Введіть вихідні дані:\n";
    std::cout << "Довжина (см) -> ";
    std::cin >> length;
    
    std::cout << "Ширина (см) -> ";
    std::cin >> width;
    
    std::cout << "Висота (см) -> ";
    std::cin >> height;
    
    double volume = length * width * height;
    
    std::cout << "Об'єм: " << volume << " куб.см." << std::endl;

    return 0;
}

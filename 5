#include <iostream>

int main() {
    int year;
    
    std::cout << "Введіть номер року: ";
    std::cin >> year;

    int days;

    if ((year % 4 == 0 && year % 100 != 0) || (year % 400 == 0)) {
        days = 366;
    } else {
        days = 365;
    }

    std::cout << "In " << year << " year = " << days << " days" << std::endl;

    return 0;
}
